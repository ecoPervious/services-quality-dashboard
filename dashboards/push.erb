<% content_for :title do %>Quality KPIs<% end %>
<div class="gridster">
  <div>Overall Services QA Metrics and KPIs</div>
  <ul>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="push_deploy_cycle" data-view="Graph" data-title="Avg Production Deploy Cycle" data-moreinfo="In Days - All Projects" data-prefix=""></div>
    </li>

    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="push_deploy_cycle_stage" data-view="Graph" data-title="Avg Staging Deploy Cycle" data-moreinfo="In Days - All Projects" data-prefix=""></div>
    </li>

    <li data-row="2" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="push_bug_cycle" data-view="Graph" data-title="Avg Bug Cycle" data-moreinfo="In Days - All Projects" data-prefix=""></div>
    </li>

    <li data-row="2" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="push_build_time" data-view="Graph" data-title="Avg Build Time" data-moreinfo="In minutes" data-prefix=""></div>
    </li>

    <li data-row="2" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="push_build_status" data-view="Number" data-title="Build Status" data-moreinfo="Green/Red" data-prefix=""></div>
    </li>

    <li data-row="2" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="push_defects_qtr" data-view="Number" data-title="Defects" data-moreinfo="Total - All Projects" data-prefix=""></div>
    </li>

    <li data-row="2" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="push_coverage" data-view="Meter" data-title="e2e Coverage" data-moreinfo="Percent - All Projects" data-prefix="" data-suffix="%" data-min="0" data-max="100"></div>
    </li>

  </ul>
</div>
